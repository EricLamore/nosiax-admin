entity ClosingRequest {
	idxAgency Integer
	closingStatus ClosingStatus
    closingDate Instant
    revokeCertificate Boolean
    revokeCertificateDate Instant
    linkEsign Boolean
    linkEsignDate Instant
    anonymized Boolean
    anonymizedDate Instant
    createBill Boolean
    createBillDate Instant
    sendBill Boolean 
    sendBillDate Instant
    terminate Boolean
    terminateDate Instant
}

entity Agency {
	idxAgency Integer
}

enum ClosingStatus {
    CREATED, SIGNED, CANCELED, DONE
}

relationship OneToOne {
	ClosingRequest{agency} to Agency
}

// Set pagination options
paginate ClosingRequest with pagination

// Use Data Transfert Objects (DTO)
// dto * with mapstruct

// Set service options to all except few
service all with serviceImpl

